<%- include('./partials/header') %>

  <table class="table table-striped table-dark">
    <tr class=" text-success">
      <th scope="col">Avatar</td>
      <th scope="col">Username</td>
      <th scope="col">Member since</td>
      <th scope="col">ID</td>
    </tr>
    <% usr.forEach(usr=> {%>
      <tr class=" text-success">
        <td>
          <img src=" <%= usr.avatar %> " alt="" class="img_tumb">
        </td>
        <th scope="row">
          <%= usr.username %>
        </th>
        <td>
          <%= usr.joined %>
        </td>
        <td>
          <%= usr._id %>
        </td>
      </tr>
      <% }); %>
  </table>
  <%- include('./partials/footer') %>