<%- include('./partials/header') %>

<table class="table table-striped table-dark">
  <tr class=" text-success">
    <th  scope="col">ID</td>
    <th  scope="col">Username</td>
    <th  scope="col">Member since</td>
  </tr>
  <% usr.forEach(usr => {%>
  <tr class=" text-success">
    <td><%= usr._id %></td>
    <th scope="row"><%= usr.username %></td>
    <td><%= usr.joined %></td>
  </tr>
  <% }); %>
</table>
<%- include('./partials/footer') %>
